{
  "name": "root",
  "version": "1.0.0",
  "description": "supreme infra setup",
  "engines": {
    "node": "24",
    "pnpm": "10"
  },
  "scripts": {
    "lint": "biome check",
    "lint:deps": "tsx infra/validate-deps/index.ts",
    "format": "pnpm run format:biome && pnpm run format:prettier",
    "format:biome": "biome check --fix",
    "format:prettier": "prettier --write \"**/*.{md,yaml,yml}\" --ignore-unknown",
    "generate:router": "tsx infra/generate/generate-router/index.ts",
    "generate:overrides": "tsx infra/generate-overrides/index.ts",
    "generate:service": "tsx infra/generate/generate-service/index.ts",
    "generate": "tsx infra/generate/index.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@inquirer/prompts": "catalog:",
    "@svgr/webpack": "catalog:",
    "@types/handlebars": "catalog:",
    "@types/inquirer": "catalog:",
    "@types/node": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "@vitest/ui": "catalog:",
    "handlebars": "catalog:",
    "inquirer": "catalog:",
    "nestjs-security-scan": "catalog:",
    "prettier": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:",
    "yaml": "catalog:"
  },
  "peerDependencies": {
    "@fastify/helmet": "^12.0.0",
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-fastify": "^11.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "next": "^15.0.0",
    "passport-jwt": "^4.0.0",
    "typeorm": "^0.3.0",
    "zod": "^3.0.0"
  },
  "author": "sewaca",
  "license": "ISC"
}
